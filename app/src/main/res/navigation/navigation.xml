<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/timeLineFragment">
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.ep3faith.profile.ProfileFragment"
        android:label="ProfileFragment" >
        <action
            android:id="@+id/action_profileFragment_to_loginActivity"
            app:destination="@id/loginActivity" />
    </fragment>
    <fragment
        android:id="@+id/timeLineFragment"
        android:name="com.example.ep3faith.timeline.TimeLineFragment"
        android:label="TimeLineFragment" />
    <fragment
        android:id="@+id/favoritesFragment"
        android:name="com.example.ep3faith.favorites.FavoritesFragment"
        android:label="FavoritesFragment" />
    <activity
        android:id="@+id/loginActivity"
        android:name="com.example.ep3faith.LoginActivity"
        android:label="LoginActivity" />
</navigation>